{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@routes/*": ["src/routes/*"],
        "@pages/*": ["src/pages/*"],
        "@components/*": ["src/components/*"],
        "@lib/*": ["src/lib/*"],
        "@partials/*": ["src/components/_partials/*"],        
        "@config/*": ["src/configApp/*"],
        "@utils/*": ["src/utils/*"]
        // Agrega más alias según sea necesario
      }
    },
    "exclude": ["node_modules", "dist", "_configenv", "_file-seg"]
}